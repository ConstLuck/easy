{
  "extends": [
    "config:base",
    ":maintainLockFilesWeekly",
    ":semanticCommits",
    ":disableDependencyDashboard",
    "github>whitesource/merge-confidence:beta"
  ],

  "updateInternalDeps": true,
  "rangeStrategy": "bump",

  "packageRules": [{
    "matchPackageNames": "next",
    "allowedVersions": "< 13",
  }, {
    "matchPackageNames": "release-it",
    "allowedVersions": "< 15",
  }]
}
